dist:v_0-004
	a                 doap:Version;
	dc:issued         "2013-05-17"^^xsd:date;
	dcs:changeset     [
		dcs:item [
			a dcs:Bugfix;
			rdfs:label "Stop claiming to be able to convert between RDF and iCalendar; right now it's only one-way.";
